PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX edam: <http://edamontology.org/> 
PREFIX bengen: <http://bengen.com/>

CONSTRUCT {
  
    ?URI rdf:type bengen:family;
	 rdfs:label ?id;
	 bengen:size ?size; 
	 bengen:belongs_to ?dataset.   
	 
  }


FROM <file:toModel-family.csv>

WHERE{
 BIND (URI(CONCAT('http://bengen.com/#', ?id ,"-in-",?dataset)) AS ?URI)
}
